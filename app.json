{
  "expo": {
    "name": "Hubodometer",
    "slug": "hubodometer-google-vision",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "android":{
      "package": "com.ai.hubodometer"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ai.hubodometer"
    }
  }
}